 <mvc:View controllerName="com.YPF.mapApp.controller.RouteDetail"
  displayBlock="true"
  xmlns="sap.m"
  xmlns:core="sap.ui.core"
  xmlns:uxap="sap.uxap"
  xmlns:f="sap.f"
  xmlns:card="sap.f.cards"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:form="sap.ui.layout.form">
  <uxap:ObjectPageLayout
    id="id_GmapPage"
    upperCaseAnchorBar="false"
    isChildPage="true"
    class="sapUiNoContentPadding">
    <uxap:headerTitle>
      <uxap:ObjectPageDynamicHeaderTitle>
        <uxap:expandedHeading>
            <Title text="{i18n>routeDetailTitle}" />
        </uxap:expandedHeading>
        <uxap:expandedContent>
          <HBox>
            <Label text="ID:"/>
            <ObjectStatus text="{RouteModel>/ROU_ID}" class="sapUiTinyMarginBegin"/>
          </HBox>
				</uxap:expandedContent>
        <uxap:snappedHeading>
          <HBox>  
            <Avatar src="data:image/png;base64,"
              class="sapUiTinyMarginEnd"
							displayShape="Square"/>
            <VBox>
              <Title text="{i18n>routeDetailTitle}" />
              <Text text="{RouteModel>/ROU_ID}"/>
            </VBox>
          </HBox>
				</uxap:snappedHeading>
        <uxap:snappedTitleOnMobile>
					<Title text="{i18n>routeDetailTitle}"/>
				</uxap:snappedTitleOnMobile>
        <uxap:actions>
          <uxap:ObjectPageHeaderActionButton icon="sap-icon://nav-back" type="Transparent" press="onNavBack" tooltip="{i18n>routeDetailBtnBack}"/>
					<Button text="Editar" type="Emphasized" />
        </uxap:actions>
      </uxap:ObjectPageDynamicHeaderTitle>
    </uxap:headerTitle>
    <uxap:headerContent>
      <!-- <Title text="Desde:" class="sapUiTinyMarginTop"/> -->
      <FlexBox wrap="Wrap" fitContainer="true">
        <Avatar src="data:image/png;base64,"
        class="sapUiMediumMarginEnd sapUiSmallMarginBottom sapUiSmallMarginTop"
        displayShape="Square"
        displaySize="XL"/>
        <VBox class="sapUiLargeMarginEnd sapUiSmallMarginTop">
          <HBox class="sapUiSmallMarginBottom" renderType="Bare">
            <Label text="ID Vehiculo:" class="sapUiTinyMarginEnd"/>
            <ObjectStatus text="{RouteModel>/VEHICLE/VEH_ID}"/>
          </HBox>
          <HBox class="sapUiSmallMarginBottom" renderType="Bare" >
            <Label text="ID Conductor:" class="sapUiTinyMarginEnd"/>
            <ObjectStatus text="{RouteModel>/DRIVER/DRI_ID}"/>
          </HBox>
          <HBox>
            <Label text="ID Cargamento:" class="sapUiTinyMarginEnd"/>
            <ObjectStatus text="{RouteModel>/SHIPMENT/SHP_ID}"/>
          </HBox>
        </VBox>
        <VBox class="sapUiLargeMarginEnd sapUiSmallMarginBottom sapUiSmallMarginTop">
					<Title text="Status" class="sapUiTinyMarginBottom" />
					<ObjectStatus  text="{path: 'RouteModel>/ROU_STATUS', formatter: '.formatter.formatRouteStatusToText'}" state="{path: 'RouteModel>/ROU_STATUS', formatter: '.formatter.formatRouteStatusToState'}"
						class="sapMObjectStatusLarge" />
        </VBox>
        <VBox class="sapUiLargeMarginEnd sapUiSmallMarginBottom sapUiSmallMarginTop">
					<Title text="Tiempo de Entrega" class="sapUiTinyMarginBottom" />
					<ObjectStatus  text="{RouteModel>/ROU_ESTIMATEDTIME} Dias"
						class="sapMObjectStatusLarge" />
        </VBox>
        <VBox class="sapUiLargeMarginEnd sapUiSmallMarginBottom sapUiSmallMarginTop">
          <Title text="Desde" class="sapUiTinyMarginBottom"/>
          <ObjectStatus text="{parts: ['RouteModel>/LOCATION_FROM/LO_COUNTRY', 'RouteModel>/LOCATION_FROM/LO_CITY']}" 	class="sapMObjectStatusLarge"/>
        </VBox>
        <VBox class="sapUiLargeMarginEnd sapUiSmallMarginBottom sapUiSmallMarginTop">
          <Title text="Hasta" class="sapUiTinyMarginBottom"/>
          <ObjectStatus text="{parts: ['RouteModel>/LOCATION_TO/LO_COUNTRY', 'RouteModel>/LOCATION_TO/LO_CITY']}" 	class="sapMObjectStatusLarge"/>
        </VBox>
      </FlexBox>
    </uxap:headerContent>
    <uxap:sections>
        <uxap:ObjectPageSection showTitle="false" title="Info Transporte">
          <uxap:subSections >
            <uxap:ObjectPageSubSection title="Transporte" showTitle="false">
              <form:SimpleForm
                editable="false"
                layout="ResponsiveGridLayout"
                labelSpanL="3"
                labelSpanM="3"
                emptySpanL="0"
                emptySpanM="0"
                columnsL="3"
                columnsM="2"
                adjustLabelSpan="false">
                <form:content>
                  <core:Title text="Vehiculo"/>
                  <Label text="ID" />
                  <ObjectStatus text="{RouteModel>/VEHICLE/VEH_ID}" />
                  <Label text="Marca" />
                  <ObjectStatus text="{RouteModel>/VEHICLE/VEH_BRAND}"/>
                  <Label text="Modelo" />
                  <ObjectStatus text="{RouteModel>/VEHICLE/VEH_MODEL}"/>
                  <Label text="Tipo" />
                  <ObjectStatus text="{RouteModel>/VEHICLE/VEH_TYPE}"/>
                  <Label text="Patente" />
                  <ObjectStatus text="{RouteModel>/VEHICLE/VEH_PATENT}"/>
                  <Label text="Status" />
                  <ObjectStatus text="{path: 'RouteModel>/VEHICLE/VEH_STATUS', formatter: '.formatter.formatVehicleStatusToText'}" state="{path: 'RouteModel>/VEHICLE/VEH_STATUS', formatter: '.formatter.formatVehicleStatusToState'}"/>
                  <core:Title text="Vehiculo"/>
                  <Label text="ID" />
                  <ObjectStatus text="{RouteModel>/DRIVER/DRI_ID}" />
                  <Label text="Nombre" />
                  <ObjectStatus text="{RouteModel>/DRIVER/DRI_NAME}"/>
                  <Label text="Apellido" />
                  <ObjectStatus text="{RouteModel>/DRIVER/DRI_LASTNAME}"/>
                  <Label text="Email" />
                  <ObjectStatus text="{RouteModel>/DRIVER/DRI_EMAIL}"/>
                  <Label text="Telefono" />
                  <ObjectStatus text="{RouteModel>/DRIVER/DRI_PHONE}"/>
                  <core:Title text="Cargamento"/>
                  <Label text="ID" />
                  <ObjectStatus text="{RouteModel>/SHIPMENT/SHP_ID}" />
                  <Label text="Tipo" />
                  <ObjectStatus text="{RouteModel>/SHIPMENT/SHP_TYPE}"/>
                  <Label text="Material" />
                  <ObjectStatus text="{RouteModel>/SHIPMENT/SHP_MATERIAL}"/>
                  <Label text="Cantidad" />
                  <ObjectStatus text="{RouteModel>/SHIPMENT/SHP_AMOUNT}"/>
                  <Label text="Proveedor" />
                  <ObjectStatus text="{RouteModel>/SHIPMENT/SHP_SUPPLIER}"/>
                </form:content>
              </form:SimpleForm>
            </uxap:ObjectPageSubSection>
          </uxap:subSections>
        </uxap:ObjectPageSection>
        <uxap:ObjectPageSection title="Direcciones" >
          <uxap:subSections >
            <uxap:ObjectPageSubSection showTitle="false" title="Rutas">
              <form:SimpleForm
                editable="false"
                layout="ResponsiveGridLayout"
                labelSpanL="3"
                labelSpanM="3"
                emptySpanL="0"
                emptySpanM="0"
                columnsL="3"
                columnsM="2"
                adjustLabelSpan="false">
                <form:content>
                  <core:Title text="Origen"/>
                  <Label text="Ciudad" />
                  <ObjectStatus text="{RouteModel>/LOCATION_FROM/LO_CITY}" />
                  <Label text="Provincia" />
                  <ObjectStatus text="{RouteModel>/LOCATION_FROM/LO_STATE}"/>
                  <Label text="Pais" />
                  <ObjectStatus text="{RouteModel>/LOCATION_FROM/LO_COUNTRY}"/>
                  <Label text="Latitud" />
                  <ObjectStatus text="{RouteModel>/LOCATION_FROM/LO_LATITUDE}"/>
                  <Label text="Longitud" />
                  <ObjectStatus text="{RouteModel>/LOCATION_FROM/LO_LONGITUDE}"/>

                  <core:Title text="Destino"/>
                  <Label text="Ciudad" />
                  <ObjectStatus text="{RouteModel>/LOCATION_TO/LO_CITY}" />
                  <Label text="Provincia" />
                  <ObjectStatus text="{RouteModel>/LOCATION_TO/LO_STATE}"/>
                  <Label text="Pais" />
                  <ObjectStatus text="{RouteModel>/LOCATION_TO/LO_COUNTRY}"/>
                  <Label text="Latitud" />
                  <ObjectStatus text="{RouteModel>/LOCATION_TO/LO_LATITUDE}"/>
                  <Label text="Longitud" />
                  <ObjectStatus text="{RouteModel>/LOCATION_TO/LO_LONGITUDE}"/>
                </form:content>
              </form:SimpleForm>
            </uxap:ObjectPageSubSection>
            <uxap:ObjectPageSubSection showTitle="false" title="Mapa">
                  <f:Card class="sapUxAPObjectPageSubSectionAlignContent">
                    <f:header>
                        <card:Header title="Info Ruta" subtitle="Salida: {RouteModel>/ROU_DEPARTURETIME} | Distancia: {RouteModel>/ROU_DISTANCE} | Tiempo estimado: {RouteModel>/ROU_ESTIMATEDTIME} Dias" />
                    </f:header>
                    <f:content>
                      <HBox height="26rem" width="100%" id="id_GMapContainer" fitContainer="true" justifyContent="Center" alignItems="Center" class="sapUiNoContentPadding">

                      </HBox>
                    </f:content>
                  </f:Card>
            </uxap:ObjectPageSubSection>
          </uxap:subSections>
        </uxap:ObjectPageSection>
    </uxap:sections>
  </uxap:ObjectPageLayout>
</mvc:View>